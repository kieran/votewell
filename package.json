    "@sentry/node": "^5.19.2",
    "@svgr/parcel-plugin-svgr": "^4.3.3",
    "@turf/tag": "^5.1.5",
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.10",
    "classnames": "^2.2.6",
    "coffeescript": "^2.5.1",
    "core-js": "^3.40.0",
    "deasync": "^0.1.30",
    "dotenv": "^8.2.0",
    "i18next": "^17.3.1",
    "koa": "^2.13.0",
    "koa-router": "^7.4.0",
    "mongodb": "^3.5.9",
