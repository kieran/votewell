{
  "name": "votewell",
  "version": "1.1.0",
  "description": "strategic voting calculator",
  "repository": "github:kieran/votewell",
  "author": "Kieran Huggins",
  "license": "ISC",
  "scripts": {
    "start": "npm-run-all -s export link -p serve api",
    "export": "export $(grep -v '^#' .env.${NODE_ENV:-development} | xargs)",
    "serve": "npx parcel serve ./index.html",
    "build": "npm-run-all -s export link dist",
    "dist": "npx parcel build --no-cache ./index.html",
    "api": "nodemon --watch data --watch ./server ./server/index.coffee",
    "up_api": "up start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "link": "rm -f ./election && ln -s ./elections/${COUNTRY:-ca} ./election",
    "update_polls": "npx coffee election/update_polls.coffee"
  },
  "dependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-react": "^7.0.0",
    "@koa/cors": "^2.2.3",
    "@sentry/browser": "^5.6.1",
    "@sentry/node": "^5.7.1",
    "@svgr/parcel-plugin-svgr": "^4.3.2",
    "@turf/tag": "^5.1.5",
    "axios": "^0.19.0",
    "cheerio": "^1.0.0-rc.3",
    "classnames": "^2.2.6",
    "coffeescript": "^2.4.0",
    "dotenv": "^8.0.0",
    "i18next": "^17.0.6",
    "koa": "^2.7.0",
    "koa-router": "^7.4.0",
    "mongodb": "^3.3.3",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.11.4",
    "react-icons": "^2.2.7",
    "react-select": "^1.2.1",
    "react-share": "^3.0.0",
    "underscore-es": "^1.9.8"
  },
  "devDependencies": {
    "mem": ">=4.0.0"
  }
}
